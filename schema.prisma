generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  department String?
  salary     Float?
  entryTime  DateTime
  exitTime   DateTime?
  position   String?
  workingOn  String?
  createdAt  DateTime @default(now())
}

